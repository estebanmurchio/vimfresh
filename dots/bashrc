# .bashrc
export EDITOR="vim"
export HISTSIZE="500"
export HISTCONTROL="ignoreboth"
export HISTIGNORE="clear:history"

# set -o vi

export PS1="\[\033[36m\]\u\[\033[m\]@\[\033[32m\]\h:\[\033[33;1m\]\w\[\033[m\]\$ "

export CLICOLOR=1
export LSCOLORS=ExFxBxDxCxegedabagacad

alias ls=' ls -GFh'
alias ll=' ls -l'
alias lala=' ls -la'
alias la=' ls -a'

alias g=' cd ~/Documents/git'

